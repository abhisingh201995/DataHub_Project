    <test name="DHD Scalability test for Performance Tier PerfTest_upto_500K-OnDemand timecardMetrics mfgold0010_int_01" enabled="true">
        <parameter name="testDescription"
                   value="Performance test to measure the scalability of the DHD infrastructure"></parameter>
        <parameter name="maxConcurrentApiCalls" value="4"/>
        <parameter name="overrideStartDate" value="2021-09-01"/>
        <parameter name="overrideEndDate" value="2021-09-05"/>
        <parameter name="performanceTier" value="PerfTest_7000-14000 - OnDemand"></parameter>
        <parameter name="tenantName" value="mfgold0010_int_01"></parameter>
        <parameter name="runType" value="Partial DR"/>
        <parameter name="pipeline" value="timecardMetrics"/>
        <parameter name="pipelinePerformanceSettings" value="[
         {'personCount':'25', 'dayCount':'7'}
          ]"/>
        <classes>
            <class name="com.ukg.datahub.perf.test.DHDArchitectureWithIncrementalDRTest"/>
        </classes>
    </test>

